{
  "name": "other-seed",
  "version": "0.1.0",
  "description": "...",
  "author": "cdcabrera",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cdcabrera/other-seed"
  },
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "run-s build:prod",
    "build:analyze": "export OUTPUT_ONLY=true; run-s build build:bundle-profile build:bundle-analyze",
    "build:bundle-profile": "webpack --config ./config/webpack.prod.js --profile --json > ./bundle.stats.json",
    "build:bundle-analyze": "webpack-bundle-analyzer ./bundle.stats.json",
    "build:clean": "rimraf ./build",
    "build:prod": "webpack --config ./config/webpack.prod.js",
    "start": "webpack serve --hot --color --progress --config ./config/webpack.dev.js",
    "test": "run-s test:type-check test:lint test:ci",
    "test:ci": "export CI=true; jest --collectCoverage",
    "test:clearCache": "jest --clearCache",
    "test:dev": "run-s test:type-check test:lint test:local",
    "test:lint": "eslint --ext .json,.js,.ts,.jsx,.tsx ./src",
    "test:lint-fix": "eslint --ext .json,.js,.ts,.jsx,.tsx ./src --fix",
    "test:local": "jest --watchAll",
    "test:type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@patternfly/patternfly": "4.70.2",
    "@patternfly/react-core": "4.84.4",
    "@patternfly/react-icons": "4.7.22",
    "@patternfly/react-styles": "4.7.22",
    "@patternfly/react-tokens": "^4.9.22",
    "node-sass": "^4.14.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.7",
    "@types/jest": "^26.0.14",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "dotenv-webpack": "^4.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "imagemin": "^7.0.0",
    "jest": "^26.5.3",
    "mini-css-extract-plugin": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.1.2",
    "raw-loader": "^4.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "sass-loader": "^7.3.1",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^6.0.0",
    "terser-webpack-plugin": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.5",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.10.3",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  }
}
